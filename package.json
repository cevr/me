{
  "private": true,
  "name": "me",
  "description": "My personal website.",
  "license": "",
  "scripts": {
    "build": "pnpm build:css && pnpm build:remix",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "postinstall": "remix setup",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@react-three/drei": "^9.8.1",
    "@react-three/fiber": "^8.0.16",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/server-runtime": "^1.4.3",
    "@remix-run/vercel": "^1.4.3",
    "autoprefixer": "^10.4.7",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "esbuild": "^0.12.29",
    "framer-motion": "^6.3.3",
    "framer-motion-3d": "^6.3.3",
    "graphql": "^16.5.0",
    "graphql-request": "^4.2.0",
    "gray-matter": "^4.0.3",
    "next-mdx-remote": "^3.0.8",
    "postcss": "^8.4.13",
    "prism-react-renderer": "^1.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "remix": "^1.4.3",
    "remix-utils": "^3.2.0",
    "tailwindcss": "^3.0.24",
    "three": "^0.140.2",
    "three-stdlib": "^2.10.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/three": "^0.140.0",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}

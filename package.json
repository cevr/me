{
  "name": "me",
  "private": true,
  "description": "My personal site.",
  "license": "",
  "scripts": {
    "cleanup": "rimraf .cache ./build ./public/build ./app/styles/tailwind.css",
    "build:remix": "remix build",
    "build:style": "cross-env NODE_ENV=production tailwindcss -o ./app/styles/tailwind.css --minify",
    "build": "npm run build:style && npm run build:remix",
    "dev:style": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "dev:remix": "remix dev",
    "dev": "concurrently \"npm:dev:*\"",
    "prebuild": "npm run cleanup",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "^0.21.0",
    "@remix-run/serve": "^0.21.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "esbuild": "^0.11.16",
    "graphql": "15",
    "graphql-request": "^3.6.1",
    "gray-matter": "^4.0.3",
    "mdx-bundler": "^8.0.0",
    "next-mdx-remote": "^3.0.8",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "remix": "^0.21.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.21.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.3.11",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
